{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": [
    "prettier"
  ],
  "env": {
    "shared-node-browser": true,
    "jest": true
  },
  "globals": {
    "window": true,
    "__webpack_require__": true
  },
  "rules": {
    "class-methods-use-this": "warn",
    "no-await-in-loop": "warn",
    "no-bitwise": "off",
    "no-param-reassign": [
      "warn",
      {
        "props": false
      }
    ],
    "no-underscore-dangle": "off",
    "prettier/prettier": [
      "error",
      {
        "singleQuote": true,
        "semi": false,
        "trailingComma": "all"
      }
    ]
  }
}